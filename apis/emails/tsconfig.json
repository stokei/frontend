{
  "extends": "@stokei/tsconfig/nextjs.json",
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"],
      "react": ["./node_modules/@types/react"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
