{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_\\*"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {},
    "codegen:generate:watch": {},
    "dev": {
      "cache": false,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
