{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {},
    "codegen:generate:watch": {},
    "dev": {
      "cache": false,
      "outputs": ["dist/**", "build/**"]
    }
  },
  "globalEnv": ["NODE_ENV", "NEXT_PUBLIC_*"]
}
